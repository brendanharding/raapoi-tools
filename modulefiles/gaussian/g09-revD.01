#%Modules
#

module-whatis "Adds Gaussian09 -A computational chemestry package - to your env"

#module load openmpi/3.1.2
#prereq openmpi/3.1.2

#module load nbo/6
#prereq nbo/6

set               root              /home/software/apps/gaussian/
set g09root	$root/g09-revD.01

set GAUSS_SCRDIR /nfs/scratch/$::env(USER)
#set GV_DIR $root/gv

# created by running, createmodule.py    $g09root/g09/bsd/g09.profile 
prepend-path 	 GAUSS_EXEDIR 		 $g09root/g09/bsd:/home/software/apps/gaussian/g09-revD.01/g09/local:/home/software/apps/gaussian/g09-revD.01/g09/extras:/home/software/apps/gaussian/g09-revD.01/g09
prepend-path 	 LD_LIBRARY_PATH 	 $g09root/g09/bsd:/home/software/apps/gaussian/g09-revD.01/g09
prepend-path 	 PATH 			 $g09root/g09:$g09root/g09/bsd

set G09BASIS 		 $g09root/g09/basis
set 		 GAUSS_ARCHDIR 		 $g09root/g09/arch
set 		 GAUSS_BSDDIR 		 $g09root/g09/bsd
set 		 GAUSS_LEXEDIR 		 $g09root/g09/linda-exe
set 		 PGI_TERM 		 trace,abort
set 		 _DSM_BARRIER 		 SHM
